{
    "manifest_version": 3,
    "name": "Evarsity Attendance Manager",
    "version": "0.0.1",
    "description": "Manage your Attendance on Evarsity",
    "permissions": ["activeTab", "tabs", "scripting", "storage"],
    "host_permissions": ["https://sp.srmist.edu.in/*"],
    "content_scripts": [
      {
        "matches": ["https://sp.srmist.edu.in/*"],
        "js": ["/scripts/contentScript.js"]
      }
    ],
    "action": {
      "default_icon": {},
      "default_title": "Attendance Manager",
      "default_popup": "/popup/popup.html"
    },
    "author": {
      "email": "yrlimbani03@gmail.com"
    }
}