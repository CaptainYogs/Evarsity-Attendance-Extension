{
    "manifest_version": 3,
    "name": "Evarsity Manager",
    "version": "0.0.1",
    "description": "Manage your Evarsity",
    "permissions": ["tabs"],
    "host_permissions": ["https://sp.srmist.edu.in/*"],
    "content_scripts": [
      {
        "matches": ["https://sp.srmist.edu.in/*"],
        "js": ["/scripts/contentScript.js"]
      }
    ],
    "icons": {
      "128": "icons/AM128.png"
    },
    "action": {
      "default_icon": {"128": "/icons/AM128.png"},
      "default_title": "Evarsity Manager",
      "default_popup": "/popup/popup.html"
    },
    "author": {
      "email": "yrlimbani03@gmail.com"
    }
}